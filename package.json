    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@types/core-js": "^0.9.43",
    "@types/node": "^8.0.50",
    "bootstrap": "^3.3.7",
    "concurrently": "^2.2.0",
    "core-js": "^2.5.1",
    "lite-server": "^2.3.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.51.1",
    "rxjs": "^5.5.2",
    "systemjs": "0.20.19",
